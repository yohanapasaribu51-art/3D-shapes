        calcInputs: [
          {name: 'base', label: 'Triangle Base (b)', unit: 'units'},
          {name: 'height', label: 'Triangle Height (h)', unit: 'units'},
          {name: 'length', label: 'Prism Length (l)', unit: 'units'}
        ],
        calcVolume: (vals) => 0.5 * vals.base * vals.height * vals.length,
        calcSA: (vals) => {
          const s = Math.sqrt(Math.pow(vals.base/2, 2) + Math.pow(vals.height, 2)) * 2;
          return vals.base * vals.height + 2 * vals.length * s + vals.length * vals.base;
        }
      },
