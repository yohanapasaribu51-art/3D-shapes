      {
        question: "If the height of a cylinder is doubled while the radius stays the same, what happens to its volume?",
        options: ["Stays the same", "Doubles", "Triples", "Quadruples"],
        correct: 1,
        difficulty: "easy",
        explanation: "V = œÄr¬≤h. If h doubles to 2h, then V becomes œÄr¬≤(2h) = 2œÄr¬≤h. Volume doubles.",
        context: "Linear relationship with height"
      },
      {
        question: "A cube and a sphere have the same volume. If the cube has edge length 10 cm, what is the approximate radius of the sphere?",
        options: ["5.42 cm", "6.20 cm", "7.81 cm", "8.95 cm"],
        correct: 1,
        difficulty: "hard",
        explanation: "Cube V = 1000 cm¬≥. Sphere: (4/3)œÄr¬≥ = 1000. Solving: r¬≥ = 750/œÄ ‚âà 238.73, so r ‚âà 6.20 cm.",
        context: "Comparing volumes across shapes"
      }
    ];

    let currentTab = 'shapes';
    let currentQuestion = 0;
    let score = 0;
    let quizStarted = false;
    let answered = false;
    let selectedAnswer = null;

    function switchTab(tab) {
      currentTab = tab;
      document.querySelectorAll('.tab-btn').forEach(btn => btn.classList.remove('active'));
      document.querySelectorAll('.tab-content').forEach(content => content.classList.remove('active'));
      
      event.target.classList.add('active');
      document.getElementById(`${tab}-tab`).classList.add('active');
    }

    function renderShapes() {
      const grid = document.getElementById('shapes-grid');
      grid.innerHTML = '';
      
      shapes.forEach(shape => {
        const card = document.createElement('div');
        card.className = 'shape-card';
        
        const shapeName = window.elementSdk.config[shape.configKey] || defaultConfig[shape.configKey];
        
        card.innerHTML = `
          <div class="shape-visual">${shape.svg}</div>
          <div class="shape-name">${shapeName}</div>
          
          <div class="properties-grid">
            <div class="property-box">
              <div class="property-label">Faces</div>
              <div class="property-value">${shape.faces}</div>
            </div>
            <div class="property-box">
              <div class="property-label">Vertices</div>
              <div class="property-value">${shape.vertices}</div>
            </div>
            <div class="property-box">
              <div class="property-label">Edges</div>
              <div class="property-value">${shape.edges}</div>
            </div>
          </div>
          
          <div class="formulas-section">
            <div class="formula-title">üìê Formulas</div>
            <div class="formula-item">
              <span class="formula-label">Volume:</span> ${shape.volumeFormula}
            </div>
            <div class="formula-item">
              <span class="formula-label">Surface Area:</span> ${shape.surfaceAreaFormula}
            </div>
          </div>
          
          <div class="key-fact">
            üí° <strong>Key Fact:</strong> ${shape.keyFact}
          </div>
        `;
        
        grid.appendChild(card);
      });
    }

    function setupCalculator() {
      const shapeSelect = document.getElementById('calc-shape');
      shapeSelect.addEventListener('change', (e) => {
        const shapeId = e.target.value;
        const shape = shapes.find(s => s.id === shapeId);
        const inputsContainer = document.getElementById('calc-inputs');
        
        if (!shape) {
          inputsContainer.innerHTML = '';
          return;
        }
        
        inputsContainer.innerHTML = shape.calcInputs.map(input => `
          <div class="form-group">
            <label class="form-label">${input.label}</label>
            <input type="number" class="form-input" id="calc-${input.name}" placeholder="Enter ${input.unit}" step="0.01" min="0">
          </div>
        `).join('');
      });
      
      document.getElementById('calculate-btn').addEventListener('click', calculateResults);
    }

    function calculateResults() {
      const shapeId = document.getElementById('calc-shape').value;
      const shape = shapes.find(s => s.id === shapeId);
      const resultDiv = document.getElementById('calc-result');
      
      if (!shape) {
        resultDiv.textContent = 'Please select a shape first';
        return;
      }
      
      const values = {};
      let allValid = true;
      
      shape.calcInputs.forEach(input => {
        const val = parseFloat(document.getElementById(`calc-${input.name}`).value);
        if (isNaN(val) || val <= 0) {
          allValid = false;
        }
        values[input.name] = val;
      });
      
      if (!allValid) {
        resultDiv.textContent = 'Please enter valid positive numbers for all dimensions';
        return;
      }
      
      const volume = shape.calcVolume(values);
      const surfaceArea = shape.calcSA(values);
      
      resultDiv.innerHTML = `
        <div style="text-align: center;">
          <div style="font-size: 18px; color: #64748b; margin-bottom: 15px;">Results for ${window.elementSdk.config[shape.configKey] || defaultConfig[shape.configKey]}</div>
          <div style="font-size: 24px; font-weight: 800; color: #667eea; margin: 10px 0;">
            Volume: ${volume.toFixed(2)} cubic units
          </div>
          <div style="font-size: 24px; font-weight: 800; color: #10b981; margin: 10px 0;">
            Surface Area: ${surfaceArea.toFixed(2)} square units
          </div>
        </div>
      `;
    }

    function startQuiz() {
      quizStarted = true;
      currentQuestion = 0;
      score = 0;
      answered = false;
      selectedAnswer = null;
      
      document.getElementById('quiz-start').style.display = 'none';
      document.getElementById('quiz-content').style.display = 'block';
      
      renderQuestion();
    }

    function renderQuestion() {
      const question = quizQuestions[currentQuestion];
      
      document.getElementById('question-counter').textContent = `Question ${currentQuestion + 1} of ${quizQuestions.length}`;
      document.getElementById('score-display').textContent = `Score: ${score}/${quizQuestions.length}`;
      
      const difficultyBadge = document.getElementById('difficulty-badge');
      difficultyBadge.textContent = question.difficulty;
      difficultyBadge.className = `difficulty-badge difficulty-${question.difficulty}`;
      
      const progress = ((currentQuestion) / quizQuestions.length) * 100;
      document.getElementById('progress-fill').style.width = `${progress}%`;
      
      document.getElementById('question-text').textContent = question.question;
      document.getElementById('question-context').textContent = question.context;
      
      const optionsGrid = document.getElementById('options-grid');
      optionsGrid.innerHTML = '';
      
      const letters = ['A', 'B', 'C', 'D'];
      question.options.forEach((option, index) => {
        const optionCard = document.createElement('div');
        optionCard.className = 'option-card';
        optionCard.innerHTML = `
          <div class="option-letter">${letters[index]}</div>
          <div>${option}</div>
        `;
        optionCard.onclick = () => selectOption(index);
        optionsGrid.appendChild(optionCard);
      });
      
      document.getElementById('feedback-box').textContent = '';
      document.getElementById('feedback-box').className = 'feedback-box empty';
      document.getElementById('submit-btn').style.display = 'inline-block';
      document.getElementById('next-btn').style.display = 'none';
      document.getElementById('submit-btn').disabled = true;
      
      answered = false;
      selectedAnswer = null;
    }

    function selectOption(index) {
      if (answered) return;
      
      selectedAnswer = index;
      
      document.querySelectorAll('.option-card').forEach((card, i) => {
        card.classList.remove('selected');
        if (i === index) {
          card.style.borderColor = '#667eea';
          card.style.background = '#f1f5f9';
        } else {
          card.style.borderColor = '#e2e8f0';
          card.style.background = 'white';
        }
      });
      
      document.getElementById('submit-btn').disabled = false;
    }

    function submitAnswer() {
      if (answered || selectedAnswer === null) return;
      
      answered = true;
      const question = quizQuestions[currentQuestion];
      const options = document.querySelectorAll('.option-card');
      const feedback = document.getElementById('feedback-box');
      
      options.forEach(opt => {
        opt.classList.add('disabled');
        opt.onclick = null;
      });
      
      if (selectedAnswer === question.correct) {
        options[selectedAnswer].classList.add('correct');
        feedback.innerHTML = `<div>‚úÖ <strong>Excellent!</strong> ${question.explanation}</div>`;
        feedback.className = 'feedback-box correct';
        score++;
      } else {
        options[selectedAnswer].classList.add('incorrect');
        options[question.correct].classList.add('correct');
        feedback.innerHTML = `<div>‚ùå <strong>Not quite.</strong> ${question.explanation}</div>`;
        feedback.className = 'feedback-box incorrect';
      }
      
      document.getElementById('submit-btn').style.display = 'none';
      document.getElementById('next-btn').style.display = 'inline-block';
      document.getElementById('score-display').textContent = `Score: ${score}/${quizQuestions.length}`;
    }

    function nextQuestion() {
      currentQuestion++;
      
      if (currentQuestion >= quizQuestions.length) {
        showResults();
      } else {
        renderQuestion();
      }
    }

    function showResults() {
      document.getElementById('quiz-content').style.display = 'none';
      
      const percentage = Math.round((score / quizQuestions.length) * 100);
      let emoji = '';
      let message = '';
      let performance = '';
      
      if (percentage >= 95) {
        emoji = 'üèÜ';
        message = 'Perfect Mastery!';
        performance = 'You have exceptional understanding of 3D geometry!';
      } else if (percentage >= 85) {
        emoji = 'üåü';
        message = 'Outstanding Performance!';
        performance = 'You have mastered Grade 10 3D shapes concepts!';
      } else if (percentage >= 75) {
        emoji = '‚≠ê';
        message = 'Great Job!';
        performance = 'You have strong understanding with room to excel!';
      } else if (percentage >= 60) {
        emoji = 'üìö';
        message = 'Good Effort!';
        performance = 'Review the study guide and practice more problems.';
      } else {
        emoji = 'üí™';
        message = 'Keep Learning!';
        performance = 'Study the formulas and try the calculator tool for practice.';
      }
      
      const resultsDiv = document.getElementById('quiz-results');
      resultsDiv.style.display = 'block';
      resultsDiv.innerHTML = `
        <div class="results-screen">
          <div class="results-emoji">${emoji}</div>
          <div class="results-score">${percentage}%</div>
          <div class="results-message">${message}</div>
          <p style="font-size: 20px; color: #64748b; margin: 20px 0; font-weight: 500;">${performance}</p>
          
          <div class="results-stats">
            <div class="stat-box">
              <div class="stat-value">${score}</div>
              <div class="stat-label">Correct</div>
            </div>
            <div class="stat-box">
              <div class="stat-value">${quizQuestions.length - score}</div>
              <div class="stat-label">Incorrect</div>
            </div>
            <div class="stat-box">
              <div class="stat-value">${quizQuestions.length}</div>
              <div class="stat-label">Total</div>
            </div>
            <div class="stat-box">
              <div class="stat-value">${percentage}%</div>
              <div class="stat-label">Score</div>
            </div>
          </div>
          
          <button class="quiz-btn secondary" onclick="restartQuiz()" id="restart-quiz-btn">
            ${window.elementSdk.config.restart_button || defaultConfig.restart_button}
          </button>
        </div>
      `;
    }

    function restartQuiz() {
      quizStarted = false;
      currentQuestion = 0;
      score = 0;
      answered = false;
      selectedAnswer = null;
      
      document.getElementById('quiz-start').style.display = 'block';
      document.getElementById('quiz-content').style.display = 'none';
      document.getElementById('quiz-results').style.display = 'none';
    }

    async function onConfigChange(config) {
      const baseSize = config.font_size || defaultConfig.font_size;
      const customFont = config.font_family || defaultConfig.font_family;
      const baseFontStack = '-apple-system, BlinkMacSystemFont, sans-serif';
      
      document.body.style.fontFamily = `${customFont}, ${baseFontStack}`;
      document.body.style.background = `linear-gradient(135deg, ${config.background_color || defaultConfig.background_color} 0%, #1e293b 25%, #334155 50%, ${config.accent_color || defaultConfig.accent_color} 100%)`;
      
      document.getElementById('app-title').textContent = config.app_title || defaultConfig.app_title;
      document.getElementById('app-title').style.fontSize = `${baseSize * 4}px`;
      
      document.getElementById('subtitle').textContent = config.subtitle || defaultConfig.subtitle;
      document.getElementById('subtitle').style.fontSize = `${baseSize * 1.5}px`;
      
      document.getElementById('calculator-title').textContent = config.calculator_title || defaultConfig.calculator_title;
      document.getElementById('calculator-title').style.fontSize = `${baseSize * 2.625}px`;
      document.getElementById('calculator-title').style.color = config.text_color || defaultConfig.text_color;
      
      document.getElementById('calculate-btn').textContent = config.calculate_button || defaultConfig.calculate_button;
      document.getElementById('calculate-btn').style.fontSize = `${baseSize * 1.25}px`;
      
      document.getElementById('quiz-title').textContent = config.quiz_title || defaultConfig.quiz_title;
      document.getElementById('quiz-title').style.fontSize = `${baseSize * 3}px`;
      document.getElementById('quiz-title').style.color = config.text_color || defaultConfig.text_color;
      
      document.getElementById('start-quiz-btn').textContent = config.start_quiz_button || defaultConfig.start_quiz_button;
      document.getElementById('start-quiz-btn').style.fontSize = `${baseSize * 1.5}px`;
      
      const submitBtn = document.getElementById('submit-btn');
      submitBtn.textContent = config.submit_button || defaultConfig.submit_button;
      submitBtn.style.fontSize = `${baseSize * 1.25}px`;
      
      const nextBtn = document.getElementById('next-btn');
      nextBtn.textContent = config.next_button || defaultConfig.next_button;
      nextBtn.style.fontSize = `${baseSize * 1.25}px`;
      
      document.querySelectorAll('.shape-card').forEach(el => {
        el.style.background = `rgba(${hexToRgb(config.surface_color || defaultConfig.surface_color)}, 0.95)`;
      });
      
      document.querySelectorAll('.shape-name').forEach(el => {
        el.style.fontSize = `${baseSize * 2}px`;
        el.style.color = config.text_color || defaultConfig.text_color;
      });
      
      document.querySelectorAll('.property-value').forEach(el => {
        el.style.color = config.accent_color || defaultConfig.accent_color;
      });
      
      document.querySelectorAll('.formula-label').forEach(el => {
        el.style.color = config.accent_color || defaultConfig.accent_color;
      });
      
      document.querySelector('.calculator-container').style.background = `rgba(${hexToRgb(config.surface_color || defaultConfig.surface_color)}, 0.95)`;
      document.querySelector('.quiz-container').style.background = `rgba(${hexToRgb(config.surface_color || defaultConfig.surface_color)}, 0.95)`;
      
      renderShapes();
    }

    function hexToRgb(hex) {
      const result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
      return result ? `${parseInt(result[1], 16)}, ${parseInt(result[2], 16)}, ${parseInt(result[3], 16)}` : '255, 255, 255';
    }

    window.elementSdk.init({
      defaultConfig,
      onConfigChange,
      mapToCapabilities: (config) => ({
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              window.elementSdk.setConfig({ background_color: value });
            }
          },
          {
            get: () => config.surface_color || defaultConfig.surface_color,
            set: (value) => {
              config.surface_color = value;
              window.elementSdk.setConfig({ surface_color: value });
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              config.text_color = value;
              window.elementSdk.setConfig({ text_color: value });
            }
          },
          {
            get: () => config.accent_color || defaultConfig.accent_color,
            set: (value) => {
              config.accent_color = value;
              window.elementSdk.setConfig({ accent_color: value });
            }
          },
          {
            get: () => config.button_color || defaultConfig.button_color,
            set: (value) => {
              config.button_color = value;
              window.elementSdk.setConfig({ button_color: value });
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            config.font_family = value;
            window.elementSdk.setConfig({ font_family: value });
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            config.font_size = value;
            window.elementSdk.setConfig({ font_size: value });
          }
        }
      }),
      mapToEditPanelValues: (config) => new Map([
        ["app_title", config.app_title || defaultConfig.app_title],
        ["subtitle", config.subtitle || defaultConfig.subtitle],
        ["cube_label", config.cube_label || defaultConfig.cube_label],
        ["sphere_label", config.sphere_label || defaultConfig.sphere_label],
        ["cylinder_label", config.cylinder_label || defaultConfig.cylinder_label],
        ["cone_label", config.cone_label || defaultConfig.cone_label],
        ["pyramid_label", config.pyramid_label || defaultConfig.pyramid_label],
        ["prism_label", config.prism_label || defaultConfig.prism_label],
        ["hemisphere_label", config.hemisphere_label || defaultConfig.hemisphere_label],
        ["rectangular_prism_label", config.rectangular_prism_label || defaultConfig.rectangular_prism_label],
        ["quiz_title", config.quiz_title || defaultConfig.quiz_title],
        ["start_quiz_button", config.start_quiz_button || defaultConfig.start_quiz_button],
        ["next_button", config.next_button || defaultConfig.next_button],
        ["submit_button", config.submit_button || defaultConfig.submit_button],
        ["restart_button", config.restart_button || defaultConfig.restart_button],
        ["calculator_title", config.calculator_title || defaultConfig.calculator_title],
        ["calculate_button", config.calculate_button || defaultConfig.calculate_button]
      ])
    });

    document.getElementById('start-quiz-btn').addEventListener('click', startQuiz);
    document.getElementById('submit-btn').addEventListener('click', submitAnswer);
    document.getElementById('next-btn').addEventListener('click', nextQuestion);

    renderShapes();
    setupCalculator();
  </script>
